                <h2 class="bc-spacing-small">Good</h2>
                <iframe width="100%" height="400px" src="https://arya-preprod.audible.com"></iframe>
            </div>
            <div class="bc-col bc-col-6 bc-col-last bc-pub-relative">
                <h2 class="bc-spacing-small">Bad</h2>
                <iframe width="100%" height="400px" src="https://www.expedia.com"></iframe>
                <div id="bad">
                <div>
            </div>
        </div>

        <script>
            document.getElementById('bad').addEventListener('click', function() {
                alert('suckaaaa');
            });
        </script>
